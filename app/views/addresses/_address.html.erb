<tr>
  <td><%= address.id %></td>
  <td><%= link_to address.user.first_name, user_path(address.user) %></td>
  <td><%= address.street_address%></td>
  <td><%= address.city.name%></td>
  <td><%= address.state.name%></td>
  <td><%= address.orders.count %></td>
  <td><%= address.created_at %></td>
  <td><%= link_to "show", user_address_path(address.user_id, address.id) %></td>
  <td><%= link_to "edit", edit_user_address_path(address.user_id, address.id) %></td>
  <td><%= link_to "delete", user_address_path(address.user_id, address.id), :method => :delete, :data => { confirm: "Are you sure?" } %></td>
</tr>